  <mrow class="MJX-TeXAtom-ORD">
    <mrow class="MJX-TeXAtom-ORD">
      <mo>+</mo>
      <mn>3</mn>
    </mrow>
  </mrow>
</math> 與<math xmlns="http://www.w3.org/1998/Math/MathML" alttext="\ce{+5}">
  <mrow class="MJX-TeXAtom-ORD">
